{
  "excluded": [
    "GridHelper",
    "PlaneGeometry",
    "BufferGeometry"
  ],
  "hashindex_prefix":"vectors_hashIndex_",
  "excludedSyncTables": ["graph", "tabs", "docs"],
  "dbName": "fileGraphDB",
  "dbStores": {
    "directories": { "keyPath": "id" },
    "files": { "keyPath": "id" },
    "graph": { "keyPath": "uuid" },
    "summaries": { "keyPath": "fileId" },
    "tabs": { "keyPath": "docId" },
    "docs": { "keyPath": "docId" },
    "peers": { "keyPath": "peerId" },
    "vectors": {
      "keyPath": "id",
      "vectorConfig": {
        "dimensions": 384,
        "hyperplanes": 10,
        "numPlanes": 5,
        "vectorPath": "embedding",
        "hashIndexSuffix": "_hashIndex"
      }
    },
    "vectors_hashIndex": {
    }
  },
  "apiConfig": {
    "endpoints": {
      "development": "http://localhost:8787/",
      "production": "https://lumi.kak2594.workers.dev/"
    },
    "models": {
      "openrouter": {
        "liquid/lfm-40b:free": {
          "provider": "OPEN",
          "endpoint": "https://openrouter.ai/api/v1/chat/completions",
          "headers": {
            "HTTP-Referer": "https://yoursite.com",
            "X-Title": "YourAppName"
          }
        },
        "google/gemma-2-9b-it": {
          "provider": "OPEN",
          "endpoint": "https://openrouter.ai/api/v1/chat/completions"
        },
        "meta-llama/llama-3-1": {
          "provider": "OPEN",
          "endpoint": "https://openrouter.ai/api/v1/chat/completions"
        }
      },
      "gemini": {
        "gemini-1.5-flash": {
          "provider": "GEMINI",
          "endpoint": "https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-flash:generateContent",
          "messageFormat": "google",
          "defaultParams": {
            "generationConfig": {
              "temperature": 1,
              "topK": 40,
              "topP": 0.95,
              "maxOutputTokens": 8192,
              "responseMimeType": "text/plain"
            }
          }
        }
      },
      "groq": {
        "mixtral-8x7b-32768": {
          "provider": "GROQ",
          "endpoint": "https://api.groq.com/openai/v1/chat/completions",
          "messageFormat": "openai"
        }
      },
      "cerebras": {
        "llama3.1-70b": {
          "provider": "CEREBRAS",
          "endpoint": "https://api.cerebras.ai/v1/chat/completions",
          "messageFormat": "openai"
        }
      }
    }
  }
}